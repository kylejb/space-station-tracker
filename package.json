{
    "name": "space-station-tracker",
    "version": "1.1.0",
    "main": "index.js",
    "private": true,
    "dependencies": {
        "axios": "^0.27.2",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "fast-xml-parser": "^4.0.7",
        "luxon": "^2.4.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.6",
        "@types/luxon": "^2.3.2",
        "@types/supertest": "^2.0.12",
        "husky": "^8.0.1",
        "jest": "^28.1.3",
        "lint-staged": "^13.0.3",
        "nock": "^13.2.9",
        "prettier": "^2.7.1",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    },
    "scripts": {
        "prepare": "node ./.husky/prepare.js",
        "heroku-prebuild": "CI=true npm run installAll",
        "build": "npx tsc && (cd web && npm run build)",
        "deps:server": "npm ci",
        "deps:web": "cd web && npm ci",
        "installAll": "npm run deps:server && npm run deps:web",
        "lint": "prettier --check .",
        "format": "prettier --write .",
        "dev": "npx ts-node index.ts",
        "start": "node build/index.js",
        "test": "npm run test:server && npm run test:web",
        "test:server": "jest",
        "test:web": "cd web && npm run test:ci"
    },
    "engines": {
        "node": "16.x"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
    }
}
