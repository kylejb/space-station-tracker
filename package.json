{
    "name": "space-station-tracker",
    "version": "1.1.0",
    "main": "index.js",
    "private": true,
    "dependencies": {
        "axios": "^1.4.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "fast-xml-parser": "^4.2.5",
        "luxon": "^3.3.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "@types/luxon": "^3.3.0",
        "@types/supertest": "^2.0.12",
        "husky": "^8.0.3",
        "jest": "^29.6.1",
        "lint-staged": "^13.2.3",
        "nock": "^13.3.2",
        "prettier": "^3.0.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "typescript": "^5.1.6"
    },
    "scripts": {
        "prepare": "node ./.husky/prepare.js",
        "heroku-prebuild": "CI=true npm run installAll",
        "build": "npx tsc && (cd web && npm run build)",
        "deps:server": "npm ci",
        "deps:web": "cd web && npm ci",
        "installAll": "npm run deps:server && npm run deps:web",
        "lint": "prettier --check .",
        "format": "prettier --write .",
        "dev": "npx ts-node index.ts",
        "start": "node build/index.js",
        "test": "npm run test:server && npm run test:web",
        "test:server": "jest",
        "test:web": "cd web && npm run test:ci"
    },
    "engines": {
        "node": ">=18.x"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
    }
}
